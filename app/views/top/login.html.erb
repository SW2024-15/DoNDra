<%= form_with url: '/top/login', method: :post, local: true do |f| %>
  <%= f.label :email, 'メールアドレス' %>
  <%= f.email_field :email %>
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  <%= f.submit 'ログイン' %>
<% end %>

<%= link_to 'ユーザー登録', new_user_path %>

<%= link_to 'ユーザー 一覧', users_path %>